<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cell_section_gray"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/a6_nagation_relative"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/aa_nagation10" >

        <Button
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:background="@drawable/af_back15"
            android:onClick="backAction" />

        <TextView
            android:id="@+id/a6_device_detail_nagation_tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="iTag"
            android:textColor="#2A983E"
            android:textSize="@dimen/sys_font_navigation" />
    </RelativeLayout>

    <View
        android:id="@+id/device_setting_headview1"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@color/cell_section_gray" />

    <RelativeLayout
         android:id="@+id/device_setting_headview2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white_color" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="@string/cell_title_36"
            android:textColor="#000000"
            android:textSize="@dimen/cell_font_commen" />
        
             <ToggleButton
                 android:id="@+id/a6_device_switch_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:background="#00000000"
        android:layout_alignParentRight="true"
        android:button="@drawable/ios7_btn"
        android:layout_marginRight="15dp"
        android:onClick="onValueChange1"
        android:text=""
        android:textOff=""
        android:textOn="" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/cell_section_gray"
        android:orientation="vertical" >

        <View
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:background="@color/clearn_color" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginLeft="15dp"
            android:layout_weight="2"
            android:text="@string/cell_title_39"
            android:textColor="@color/sys_gray_color"
            android:textSize="@dimen/cell_font_title" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white_color" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="@string/cell_title_40"
            android:textColor="#000000"
            android:textSize="@dimen/cell_font_commen" />
        
             <ToggleButton
                 android:id="@+id/a6_device_switch_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:background="#00000000"
        android:layout_alignParentRight="true"
        android:button="@drawable/ios7_btn"
        android:layout_marginRight="15dp"
        android:onClick="onValueChange2"
        android:text=""
        android:textOff=""
        android:textOn="" />
        
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/cell_section_gray"
        android:orientation="vertical" >

        <View
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:background="@color/clearn_color" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginLeft="15dp"
            android:layout_weight="2"
            android:text="@string/cell_title_41"
            android:textColor="@color/sys_gray_color"
            android:textSize="@dimen/cell_font_title" />
    </LinearLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/white_color" >

        <ImageView
            android:id="@+id/a6_device_setting_low"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:src="@drawable/ak_smallvoice_21" />
        
        <SeekBar 
            android:id="@+id/a6_device_seekbar_3"
            android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			 android:layout_centerVertical="true"  
			 android:layout_marginLeft="10dp"
			 android:layout_marginRight="10dp"
			 android:layout_toRightOf="@+id/a6_device_setting_low"   
            android:layout_toLeftOf="@+id/a6_device_setting_up"
            android:background="@color/lightgray_color"
             android:progressDrawable="@drawable/seekbar_backgrou_1"  
        android:thumb="@drawable/seekbar_thumb_2"
            android:max="10"
            
            />

        <ImageView
            android:id="@+id/a6_device_setting_up"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:src="@drawable/al_bigvoice_22" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/lightgray_color" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:onClick="alamSettingAction"
        android:background="@drawable/selector_device_list3" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="@string/cell_title_42"
            android:textColor="#000000"
            android:textSize="@dimen/cell_font_commen" />
        
          <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:src="@drawable/ad_arr13" />
    </RelativeLayout>

</LinearLayout>