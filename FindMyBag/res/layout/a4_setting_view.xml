<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cell_section_gray"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/nagation_relative"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/aa_nagation10" >

        <TextView
            android:id="@+id/device_nagation_tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/tabbar_settings_4"
            android:textColor="@color/sys_green_color"
            android:textSize="@dimen/sys_font_navigation" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@color/cell_section_gray" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#FFFFFF"
         >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="@string/cell_title_9"
            android:textColor="@color/sys_gray_color"
            android:textSize="@dimen/cell_font_commen" />
       
           <ToggleButton
               android:id="@+id/a4_setting_view_togglebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:background="#00000000"
        android:layout_alignParentRight="true"
        android:button="@drawable/ios7_btn"
        android:layout_marginRight="15dp"
        android:checked="false"
        android:text=""
        android:textOff=""
        android:textOn="" />
       
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/lightgray_color" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/selector_device_list3"
        android:onClick="clickAction"
        android:tag="1" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="@string/cell_title_10"
            android:textColor="@color/sys_gray_color"
            android:textSize="@dimen/cell_font_commen" />

        <TextView
            android:id="@+id/a4_setting_view_password_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp"
            android:text="@string/cell_title_14_a"
            android:textColor="@color/lightgray_color"
            android:textSize="12sp" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:src="@drawable/ad_arr13" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/lightgray_color" />

    <View
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@color/cell_section_gray" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/selector_device_list3"
        android:onClick="clickAction"
        android:tag="2" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="@string/cell_title_11"
            android:textColor="@color/sys_gray_color"
            android:textSize="@dimen/cell_font_commen" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:src="@drawable/ad_arr13" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/lightgray_color" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/selector_device_list3"
        android:onClick="clickAction"
        android:tag="3" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="@string/cell_title_12"
            android:textColor="@color/sys_gray_color"
            android:textSize="@dimen/cell_font_commen" />

        <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="15dp"
            android:src="@drawable/ad_arr13" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/lightgray_color" />

    <View
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@color/cell_section_gray" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#FFFFFF"
         >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:text="@string/cell_title_13"
            android:textColor="@color/sys_gray_color"
            android:textSize="@dimen/cell_font_commen" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="25dp"
            android:text="V 2.2.3"
           android:textColor="@color/lightgray_color"
            android:textSize="12sp" />
    </RelativeLayout>

     

</LinearLayout>